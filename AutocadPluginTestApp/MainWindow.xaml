<Window x:Class="AutocadPluginTestApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:helpers ="clr-namespace:AutocadPluginTestApp.Helpers"
        xmlns:views="clr-namespace:LayerPlugin.Views;assembly=LayerPlugin.Views"
        mc:Ignorable="d"
        Title="Autocad Layer Plugin Test App" Height="350" Width="725">
    <Grid>
        <Button Command="{Binding StartPluginCommand}"
                Content="Start Plugin"
                Height="50" Width="200"
                Margin="10"
                HorizontalAlignment="Left" VerticalAlignment="Top"
                />
    </Grid>
</Window>
